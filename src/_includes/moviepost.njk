  <!DOCTYPE html>
  <html lang="en" class="js">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="tilte" content="{{ seoTitle }}" >
    <meta name="description" content="{{ seoDescription }}" >
    <meta name="author" content="{{ author }}" >
    <meta name="keywords" content="{{ seoKeywords }} , {{ leadActors }}" >
    <title>Net Blog | {{ title | title }} </title>
    <link rel="stylesheet" href="/styles.css">
    <link rel="stylesheet" href="/vendor.css">
    </head>
  <body id="top">    
    <div id="page" class="s-pagewrap ss-home">
            {% include "header.njk" %} 
                 
          <section id="content" class="s-content">          
            <div id="bricks" class="bricks">            
              <div class="masonry">             
                <div class="bricks-wrapper" data-animate-block>   
                <div class="grid-sizer"></div>
                  <article class="brick entry" data-animate-el>                  
                    <div class="entry__thumb">
                    <h2 class="headings" >Download {{ movieName | title }} </h2>
                      <a href="{{ post.url }}" class="thumb-link"> 
                        <img class="movieimage" href="{{ post.url }}" src="{{ image }}" alt="{{ imageAlt | title }}">
                      </a>
                    </div> 
                  <div class="entry__text">
                    <div class="entry__header"> 
                      <div class="entry__meta"> 
                        <span class="cat-links"> 
                          <a href="/category/{{ category | lower }}/" >{{ category }}</a>
                        </span> 
                        <span class="byline"> 
                             By: <a href="#0">{{ author | title }} on {{ date | postDate }}</a>
                        </span> 
                      </div>                         
                    </div>                      
                  </div> 
                  <div class="entry__text">                              
    Download {{ movieName | title }} Released On {{ releaseDate }} in {{ maxQuality }} in {{ languageAvailable | capitalize }}
    </div>               
</article>
                </div>
                {{ content | safe }}
              </div>
              <a href="{{ movieLink }}" class="the-link" target="_blank" >Download {{ movieName | title }}</a>
            </div>
          </section>
            {% include "footer.njk" %}
          </div>
        <script src="/plugins.js"></script>
        <script src="/main.js"></script>        
        </body>
      </html>
